<Window x:Class="Assignment_4._1._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Contact Search" Height="400" Width="600">
    <Grid>
        <TextBox Name="txtSearch" Width="200" Height="30" Margin="10" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Button Content="Search" Width="100" Height="30" Margin="220,10,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Click="SearchButtonClick" />
        <DataGrid Name="ContactData" Margin="10,50,10,150" AutoGenerateColumns="True" />
        <DataGrid Name="datagrid2" Margin="10,250,10,10" AutoGenerateColumns="True" />
    </Grid>
</Window>
